{"ast":null,"code":"import axios from 'axios'; // Importar Axios\n\nexport default {\n  data() {\n    return {\n      first_name: '',\n      last_name: '',\n      email: '',\n      phone: '',\n      password: '',\n      confirm_password: '',\n      acceptsPolicy: false // Nuevo campo para aceptar la política de datos\n    };\n  },\n  methods: {\n    async register() {\n      if (!this.acceptsPolicy) {\n        alert('Debes aceptar la política de tratamiento de datos.');\n        return;\n      }\n      if (this.password !== this.confirm_password) {\n        alert('Las contraseñas no coinciden.');\n        return;\n      }\n      try {\n        const response = await axios.post('http://localhost:8000/api/register', {\n          first_name: this.first_name,\n          last_name: this.last_name,\n          email: this.email,\n          phone: this.phone,\n          password: this.password,\n          accepts_policy: this.acceptsPolicy // Enviar el valor al backend\n        });\n        if (response.status === 200) {\n          alert('Usuario registrado con éxito');\n        } else {\n          alert('Error: ' + response.data.message);\n        }\n      } catch (error) {\n        console.error('Error:', error);\n        alert('Error de servidor');\n      }\n    }\n  }\n};","map":{"version":3,"names":["axios","data","first_name","last_name","email","phone","password","confirm_password","acceptsPolicy","methods","register","alert","response","post","accepts_policy","status","message","error","console"],"sources":["/home/infraestructura/Documents/UNIVERSIDAD/SEMESTRE 5/Programacion orientada en web/proyecto_alojamiento/alojamiento-frontends/src/components/RegisterComponent.vue"],"sourcesContent":["<template>\n  <div class=\"register\">\n    <h2>Register</h2>\n    <form @submit.prevent=\"register\">\n      <!-- Nombre -->\n      <div>\n        <label for=\"first_name\">Nombre</label>\n        <input type=\"text\" v-model=\"first_name\" required />\n      </div>\n\n      <!-- Apellido -->\n      <div>\n        <label for=\"last_name\">Apellido</label>\n        <input type=\"text\" v-model=\"last_name\" required />\n      </div>\n\n      <!-- Correo electrónico -->\n      <div>\n        <label for=\"email\">Correo electrónico</label>\n        <input type=\"email\" v-model=\"email\" required />\n      </div>\n\n      <!-- Número de teléfono -->\n      <div>\n        <label for=\"phone\">Número de teléfono</label>\n        <input type=\"tel\" v-model=\"phone\" required />\n      </div>\n\n      <!-- Contraseña -->\n      <div>\n        <label for=\"password\">Contraseña</label>\n        <input type=\"password\" v-model=\"password\" required />\n      </div>\n\n      <!-- Confirmar contraseña -->\n      <div>\n        <label for=\"confirm_password\">Confirmar contraseña</label>\n        <input type=\"password\" v-model=\"confirm_password\" required />\n      </div>\n\n      <!-- Casilla para aceptar la política de tratamiento de datos -->\n      <div>\n        <label>\n          <input type=\"checkbox\" v-model=\"acceptsPolicy\" />\n          Acepto la política de tratamiento de datos\n        </label>\n      </div>\n\n      <button type=\"submit\">Registrar</button>\n    </form>\n  </div>\n</template>\n\n<script>\nimport axios from 'axios'; // Importar Axios\n\nexport default {\n  data() {\n    return {\n      first_name: '',\n      last_name: '',\n      email: '',\n      phone: '',\n      password: '',\n      confirm_password: '',\n      acceptsPolicy: false, // Nuevo campo para aceptar la política de datos\n    };\n  },\n  methods: {\n    async register() {\n      if (!this.acceptsPolicy) {\n        alert('Debes aceptar la política de tratamiento de datos.');\n        return;\n      }\n\n      if (this.password !== this.confirm_password) {\n        alert('Las contraseñas no coinciden.');\n        return;\n      }\n\n      try {\n        const response = await axios.post('http://localhost:8000/api/register', {\n          first_name: this.first_name,\n          last_name: this.last_name,\n          email: this.email,\n          phone: this.phone,\n          password: this.password,\n          accepts_policy: this.acceptsPolicy, // Enviar el valor al backend\n        });\n\n        if (response.status === 200) {\n          alert('Usuario registrado con éxito');\n        } else {\n          alert('Error: ' + response.data.message);\n        }\n      } catch (error) {\n        console.error('Error:', error);\n        alert('Error de servidor');\n      }\n    },\n  },\n};\n</script>\n\n\n<style scoped>\n* {\n  margin: 0;\n  padding: 0;\n  box-sizing: border-box;\n}\n\nbody {\n  font-family: Arial, sans-serif;\n  background-image: url('/../proyecto_alojamiento/alojamiento-frontends/src/assets/img_hotel.jpeg');\n  background-size: cover;\n  background-position: center;\n  display: flex;\n  justify-content: center;\n  align-items: center;\n  height: 100vh;\n}\n\n.container {\n  background-color: rgba(255, 255, 255, 0.9);\n  padding: 30px;\n  border-radius: 8px;\n  box-shadow: 0 4px 10px rgba(0, 0, 0, 0.1);\n  width: 100%;\n  max-width: 400px;\n}\n\nh2 {\n  text-align: center;\n  margin-bottom: 20px;\n  color: #333;\n}\n\n.form-register {\n  display: flex;\n  flex-direction: column;\n}\n\n.form-group {\n  margin-bottom: 15px;\n}\n\nlabel {\n  margin-bottom: 5px;\n  font-weight: bold;\n  font-size: 14px;\n  color: #555;\n}\n\ninput[type=\"text\"],\ninput[type=\"email\"],\ninput[type=\"tel\"],\ninput[type=\"password\"] {\n  padding: 10px;\n  font-size: 16px;\n  border: 1px solid #ccc;\n  border-radius: 4px;\n  width: 100%;\n  transition: border-color 0.3s;\n}\n\ninput[type=\"text\"]:focus,\ninput[type=\"email\"]:focus,\ninput[type=\"tel\"]:focus,\ninput[type=\"password\"]:focus {\n  border-color: #007bff;\n  outline: none;\n}\n\ninput[type=\"checkbox\"] {\n  margin-right: 10px;\n}\n\n.btn-register {\n  background-color: #007bff;\n  color: white;\n  padding: 10px;\n  font-size: 16px;\n  border: none;\n  border-radius: 4px;\n  cursor: pointer;\n  transition: background-color 0.3s;\n}\n\n.btn-register:hover {\n  background-color: #0056b3;\n}\n</style>\n"],"mappings":"AAsDA,OAAOA,KAAI,MAAO,OAAO,EAAE;;AAE3B,eAAe;EACbC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,UAAU,EAAE,EAAE;MACdC,SAAS,EAAE,EAAE;MACbC,KAAK,EAAE,EAAE;MACTC,KAAK,EAAE,EAAE;MACTC,QAAQ,EAAE,EAAE;MACZC,gBAAgB,EAAE,EAAE;MACpBC,aAAa,EAAE,KAAK,CAAE;IACxB,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACP,MAAMC,QAAQA,CAAA,EAAG;MACf,IAAI,CAAC,IAAI,CAACF,aAAa,EAAE;QACvBG,KAAK,CAAC,oDAAoD,CAAC;QAC3D;MACF;MAEA,IAAI,IAAI,CAACL,QAAO,KAAM,IAAI,CAACC,gBAAgB,EAAE;QAC3CI,KAAK,CAAC,+BAA+B,CAAC;QACtC;MACF;MAEA,IAAI;QACF,MAAMC,QAAO,GAAI,MAAMZ,KAAK,CAACa,IAAI,CAAC,oCAAoC,EAAE;UACtEX,UAAU,EAAE,IAAI,CAACA,UAAU;UAC3BC,SAAS,EAAE,IAAI,CAACA,SAAS;UACzBC,KAAK,EAAE,IAAI,CAACA,KAAK;UACjBC,KAAK,EAAE,IAAI,CAACA,KAAK;UACjBC,QAAQ,EAAE,IAAI,CAACA,QAAQ;UACvBQ,cAAc,EAAE,IAAI,CAACN,aAAa,CAAE;QACtC,CAAC,CAAC;QAEF,IAAII,QAAQ,CAACG,MAAK,KAAM,GAAG,EAAE;UAC3BJ,KAAK,CAAC,8BAA8B,CAAC;QACvC,OAAO;UACLA,KAAK,CAAC,SAAQ,GAAIC,QAAQ,CAACX,IAAI,CAACe,OAAO,CAAC;QAC1C;MACF,EAAE,OAAOC,KAAK,EAAE;QACdC,OAAO,CAACD,KAAK,CAAC,QAAQ,EAAEA,KAAK,CAAC;QAC9BN,KAAK,CAAC,mBAAmB,CAAC;MAC5B;IACF;EACF;AACF,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}