{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport axios from 'axios';\nexport default {\n  name: 'RegisterComponent',\n  data() {\n    return {\n      username: '',\n      email: '',\n      password: '',\n      acceptTerms: false\n    };\n  },\n  methods: {\n    async registerUser() {\n      if (!this.acceptTerms) {\n        alert(\"Debes aceptar las políticas de privacidad.\");\n        return;\n      }\n      try {\n        await axios.post('http://localhost:5000/api/auth/register', {\n          username: this.username,\n          email: this.email,\n          password: this.password,\n          accept_terms: this.acceptTerms\n        });\n        // Redirigir al login después de registrarse\n        this.$router.push('/login');\n      } catch (error) {\n        console.error('Error al registrar usuario:', error);\n      }\n    }\n  }\n};","map":{"version":3,"names":["axios","name","data","username","email","password","acceptTerms","methods","registerUser","alert","post","accept_terms","$router","push","error","console"],"sources":["/home/infraestructura/Documents/UNIVERSIDAD/SEMESTRE 5/Programacion orientada en web/proyecto_alojamiento/alojamiento-frontends/src/components/RegisterComponent.vue"],"sourcesContent":["<template>\n  <div class=\"register\">\n    <h2>Registro de Usuario</h2>\n    <form @submit.prevent=\"registerUser\">\n      <div>\n        <label for=\"username\">Nombre de Usuario:</label>\n        <input type=\"text\" v-model=\"username\" required />\n      </div>\n      <div>\n        <label for=\"email\">Correo Electrónico:</label>\n        <input type=\"email\" v-model=\"email\" required />\n      </div>\n      <div>\n        <label for=\"password\">Contraseña:</label>\n        <input type=\"password\" v-model=\"password\" required />\n      </div>\n      <div>\n        <input type=\"checkbox\" v-model=\"acceptTerms\" />\n        <label for=\"terms\">Acepto las políticas de privacidad</label>\n      </div>\n      <button type=\"submit\">Registrarse</button>\n    </form>\n    <p>¿Ya tienes una cuenta? <router-link to=\"/login\">Inicia sesión aquí</router-link></p>\n  </div>\n</template>\n\n<script>\nimport axios from 'axios';\n\nexport default {\n  name: 'RegisterComponent',\n  data() {\n    return {\n      username: '',\n      email: '',\n      password: '',\n      acceptTerms: false\n    };\n  },\n  methods: {\n    async registerUser() {\n      if (!this.acceptTerms) {\n        alert(\"Debes aceptar las políticas de privacidad.\");\n        return;\n      }\n\n      try {\n        await axios.post('http://localhost:5000/api/auth/register', {\n          username: this.username,\n          email: this.email,\n          password: this.password,\n          accept_terms: this.acceptTerms\n        });\n        // Redirigir al login después de registrarse\n        this.$router.push('/login');\n      } catch (error) {\n        console.error('Error al registrar usuario:', error);\n      }\n    }\n  }\n};\n</script>\n\n<style scoped>\n/* Estilos para el formulario de registro */\n.register {\n  max-width: 400px;\n  margin: auto;\n}\n</style>\n\n"],"mappings":";AA2BA,OAAOA,KAAI,MAAO,OAAO;AAEzB,eAAe;EACbC,IAAI,EAAE,mBAAmB;EACzBC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,QAAQ,EAAE,EAAE;MACZC,KAAK,EAAE,EAAE;MACTC,QAAQ,EAAE,EAAE;MACZC,WAAW,EAAE;IACf,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACP,MAAMC,YAAYA,CAAA,EAAG;MACnB,IAAI,CAAC,IAAI,CAACF,WAAW,EAAE;QACrBG,KAAK,CAAC,4CAA4C,CAAC;QACnD;MACF;MAEA,IAAI;QACF,MAAMT,KAAK,CAACU,IAAI,CAAC,yCAAyC,EAAE;UAC1DP,QAAQ,EAAE,IAAI,CAACA,QAAQ;UACvBC,KAAK,EAAE,IAAI,CAACA,KAAK;UACjBC,QAAQ,EAAE,IAAI,CAACA,QAAQ;UACvBM,YAAY,EAAE,IAAI,CAACL;QACrB,CAAC,CAAC;QACF;QACA,IAAI,CAACM,OAAO,CAACC,IAAI,CAAC,QAAQ,CAAC;MAC7B,EAAE,OAAOC,KAAK,EAAE;QACdC,OAAO,CAACD,KAAK,CAAC,6BAA6B,EAAEA,KAAK,CAAC;MACrD;IACF;EACF;AACF,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}