{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport axios from 'axios'; // Importar Axios\n\nexport default {\n  data() {\n    return {\n      email: '',\n      password: ''\n    };\n  },\n  methods: {\n    goToHome() {\n      this.$router.push('/home');\n    },\n    async login() {\n      try {\n        const response = await axios.post('http://localhost:8080/v1/users/login', {\n          email: this.email,\n          password: this.password\n        });\n        if (response.status === 200) {\n          alert('Inicio de sesión exitoso');\n        } else {\n          alert('Error: ' + response.data.message);\n        }\n      } catch (error) {\n        console.error('Error:', error);\n        alert('Error de servidor');\n      }\n    }\n  }\n};","map":{"version":3,"names":["axios","data","email","password","methods","goToHome","$router","push","login","response","post","status","alert","message","error","console"],"sources":["/home/infraestructura/Documents/UNIVERSIDAD/SEMESTRE 5/Programacion orientada en web/proyecto_alojamiento/alojamiento-frontends/src/components/LoginComponent.vue"],"sourcesContent":["<template>\n  <div class=\"right-bar\">\n      <nav class=\"navbar\">\n        <button @click=\"goToHome\" class=\"btn2\">Inicio</button>\n      </nav>\n    </div>\n  <div class=\"container login\">\n    <h2>Login</h2>\n    <form class=\"form-login\" @submit.prevent=\"login\">\n      <!-- Correo electrónico -->\n      <div class=\"form-group\">\n        <label for=\"email\">Correo electrónico</label>\n        <input type=\"email\" v-model=\"email\" required />\n      </div>\n\n      <!-- Contraseña -->\n      <div class=\"form-group\">\n        <label for=\"password\">Contraseña</label>\n        <input type=\"password\" v-model=\"password\" required />\n      </div>\n\n      <button type=\"submit\" class=\"btn-login\">Iniciar sesión</button>\n    </form>\n    <p class=\"register-link\">¿Aun no tienes una cuenta? <a href=\"/register\">Regístrate</a></p>\n  </div>\n</template>\n\n<script>\nimport axios from 'axios'; // Importar Axios\n\nexport default {\n  data() {\n    return {\n      email: '',\n      password: '',\n    };\n  },\n  methods: {\n    goToHome() {\n      this.$router.push('/home');\n    },\n    async login() {\n      try {\n        const response = await axios.post('http://localhost:8080/v1/users/login', {\n          email: this.email,\n          password: this.password,\n        });\n\n        if (response.status === 200) {\n          alert('Inicio de sesión exitoso');\n        } else {\n          alert('Error: ' + response.data.message);\n        }\n      } catch (error) {\n        console.error('Error:', error);\n        alert('Error de servidor');\n      }\n    },\n  },\n};\n</script>\n\n<style>\n@import '@/assets/styly.css';\n</style>"],"mappings":";AA4BA,OAAOA,KAAI,MAAO,OAAO,EAAE;;AAE3B,eAAe;EACbC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,KAAK,EAAE,EAAE;MACTC,QAAQ,EAAE;IACZ,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACPC,QAAQA,CAAA,EAAG;MACT,IAAI,CAACC,OAAO,CAACC,IAAI,CAAC,OAAO,CAAC;IAC5B,CAAC;IACD,MAAMC,KAAKA,CAAA,EAAG;MACZ,IAAI;QACF,MAAMC,QAAO,GAAI,MAAMT,KAAK,CAACU,IAAI,CAAC,sCAAsC,EAAE;UACxER,KAAK,EAAE,IAAI,CAACA,KAAK;UACjBC,QAAQ,EAAE,IAAI,CAACA;QACjB,CAAC,CAAC;QAEF,IAAIM,QAAQ,CAACE,MAAK,KAAM,GAAG,EAAE;UAC3BC,KAAK,CAAC,0BAA0B,CAAC;QACnC,OAAO;UACLA,KAAK,CAAC,SAAQ,GAAIH,QAAQ,CAACR,IAAI,CAACY,OAAO,CAAC;QAC1C;MACF,EAAE,OAAOC,KAAK,EAAE;QACdC,OAAO,CAACD,KAAK,CAAC,QAAQ,EAAEA,KAAK,CAAC;QAC9BF,KAAK,CAAC,mBAAmB,CAAC;MAC5B;IACF;EACF;AACF,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}