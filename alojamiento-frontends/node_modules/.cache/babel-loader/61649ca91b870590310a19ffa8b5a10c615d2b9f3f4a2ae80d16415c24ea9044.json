{"ast":null,"code":"import axios from 'axios';\nexport default {\n  data() {\n    return {\n      first_name: '',\n      last_name: '',\n      email: '',\n      phone: '',\n      password: '',\n      confirm_password: '',\n      acceptsPolicy: false // Nuevo campo para aceptar la política de datos\n    };\n  },\n  methods: {\n    async register() {\n      if (!this.acceptsPolicy) {\n        alert('Debes aceptar la política de tratamiento de datos.');\n        return;\n      }\n      if (this.password !== this.confirm_password) {\n        alert('Las contraseñas no coinciden.');\n        return;\n      }\n      try {\n        const response = await fetch('http://localhost:8000/api/register', {\n          method: 'POST',\n          headers: {\n            'Content-Type': 'application/json'\n          },\n          body: JSON.stringify({\n            first_name: this.first_name,\n            last_name: this.last_name,\n            email: this.email,\n            phone: this.phone,\n            password: this.password,\n            accepts_policy: this.acceptsPolicy // Enviar el valor al backend\n          })\n        });\n        if (response.ok) {\n          alert('Usuario registrado con éxito');\n        } else {\n          const error = await response.json();\n          alert('Error: ' + error.message);\n        }\n      } catch (error) {\n        console.error('Error:', error);\n        alert('Error de servidor');\n      }\n    }\n  }\n};","map":{"version":3,"names":["axios","data","first_name","last_name","email","phone","password","confirm_password","acceptsPolicy","methods","register","alert","response","fetch","method","headers","body","JSON","stringify","accepts_policy","ok","error","json","message","console"],"sources":["/home/infraestructura/Documents/UNIVERSIDAD/SEMESTRE 5/Programacion orientada en web/proyecto_alojamiento/alojamiento-frontends/src/components/RegisterComponent.vue"],"sourcesContent":["<template>\n  <div class=\"register\">\n    <h2>Register</h2>\n    <form @submit.prevent=\"register\">\n      <!-- Nombre -->\n      <div>\n        <label for=\"first_name\">Nombre</label>\n        <input type=\"text\" v-model=\"first_name\" required />\n      </div>\n\n      <!-- Apellido -->\n      <div>\n        <label for=\"last_name\">Apellido</label>\n        <input type=\"text\" v-model=\"last_name\" required />\n      </div>\n\n      <!-- Correo electrónico -->\n      <div>\n        <label for=\"email\">Correo electrónico</label>\n        <input type=\"email\" v-model=\"email\" required />\n      </div>\n\n      <!-- Número de teléfono -->\n      <div>\n        <label for=\"phone\">Número de teléfono</label>\n        <input type=\"tel\" v-model=\"phone\" required />\n      </div>\n\n      <!-- Contraseña -->\n      <div>\n        <label for=\"password\">Contraseña</label>\n        <input type=\"password\" v-model=\"password\" required />\n      </div>\n\n      <!-- Confirmar contraseña -->\n      <div>\n        <label for=\"confirm_password\">Confirmar contraseña</label>\n        <input type=\"password\" v-model=\"confirm_password\" required />\n      </div>\n\n      <!-- Casilla para aceptar la política de tratamiento de datos -->\n      <div>\n        <label>\n          <input type=\"checkbox\" v-model=\"acceptsPolicy\" />\n          Acepto la política de tratamiento de datos\n        </label>\n      </div>\n\n      <button type=\"submit\">Registrar</button>\n    </form>\n  </div>\n</template>\n\n<script>\nimport axios from 'axios';\nexport default {\n  data() {\n    return {\n      first_name: '',\n      last_name: '',\n      email: '',\n      phone: '',\n      password: '',\n      confirm_password: '',\n      acceptsPolicy: false, // Nuevo campo para aceptar la política de datos\n    };\n  },\n  methods: {\n    async register() {\n      if (!this.acceptsPolicy) {\n        alert('Debes aceptar la política de tratamiento de datos.');\n        return;\n      }\n\n      if (this.password !== this.confirm_password) {\n        alert('Las contraseñas no coinciden.');\n        return;\n      }\n\n      try {\n        const response = await fetch('http://localhost:8000/api/register', {\n          method: 'POST',\n          headers: {\n            'Content-Type': 'application/json',\n          },\n          body: JSON.stringify({\n            first_name: this.first_name,\n            last_name: this.last_name,\n            email: this.email,\n            phone: this.phone,\n            password: this.password,\n            accepts_policy: this.acceptsPolicy, // Enviar el valor al backend\n          }),\n        });\n\n        if (response.ok) {\n          alert('Usuario registrado con éxito');\n        } else {\n          const error = await response.json();\n          alert('Error: ' + error.message);\n        }\n      } catch (error) {\n        console.error('Error:', error);\n        alert('Error de servidor');\n      }\n    },\n  },\n};\n</script>\n\n<style scoped>\n/* Agrega los estilos que necesites */\n</style>\n"],"mappings":"AAsDA,OAAOA,KAAI,MAAO,OAAO;AACzB,eAAe;EACbC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,UAAU,EAAE,EAAE;MACdC,SAAS,EAAE,EAAE;MACbC,KAAK,EAAE,EAAE;MACTC,KAAK,EAAE,EAAE;MACTC,QAAQ,EAAE,EAAE;MACZC,gBAAgB,EAAE,EAAE;MACpBC,aAAa,EAAE,KAAK,CAAE;IACxB,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACP,MAAMC,QAAQA,CAAA,EAAG;MACf,IAAI,CAAC,IAAI,CAACF,aAAa,EAAE;QACvBG,KAAK,CAAC,oDAAoD,CAAC;QAC3D;MACF;MAEA,IAAI,IAAI,CAACL,QAAO,KAAM,IAAI,CAACC,gBAAgB,EAAE;QAC3CI,KAAK,CAAC,+BAA+B,CAAC;QACtC;MACF;MAEA,IAAI;QACF,MAAMC,QAAO,GAAI,MAAMC,KAAK,CAAC,oCAAoC,EAAE;UACjEC,MAAM,EAAE,MAAM;UACdC,OAAO,EAAE;YACP,cAAc,EAAE;UAClB,CAAC;UACDC,IAAI,EAAEC,IAAI,CAACC,SAAS,CAAC;YACnBhB,UAAU,EAAE,IAAI,CAACA,UAAU;YAC3BC,SAAS,EAAE,IAAI,CAACA,SAAS;YACzBC,KAAK,EAAE,IAAI,CAACA,KAAK;YACjBC,KAAK,EAAE,IAAI,CAACA,KAAK;YACjBC,QAAQ,EAAE,IAAI,CAACA,QAAQ;YACvBa,cAAc,EAAE,IAAI,CAACX,aAAa,CAAE;UACtC,CAAC;QACH,CAAC,CAAC;QAEF,IAAII,QAAQ,CAACQ,EAAE,EAAE;UACfT,KAAK,CAAC,8BAA8B,CAAC;QACvC,OAAO;UACL,MAAMU,KAAI,GAAI,MAAMT,QAAQ,CAACU,IAAI,C